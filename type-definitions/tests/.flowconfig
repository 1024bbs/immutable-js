[libs]
../immutable.js.flow

[ignore]
; Unless node_modules are ignored, 'immutable' resolves to the module not the flow definition.
.*/node_modules/.*

[options]
; Prevents Flow from reporting an error on a line which follows `// $ExpectError` comment.
suppress_comment=\\(.\\|\n\\)*\\$ExpectError

[version]
^0.36.0
