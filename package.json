  "engines": {
    "node": ">=0.8.0"
  },
  "files": [
    "dist",
    "contrib",
    "README.md",
    "LICENSE",
    "PATENTS"
  ],
  "keywords": [
    "immutable",
    "persistent",
    "lazy",
    "data",
    "datastructure",
    "functional",
    "collection",
    "stateless",
    "sequence",
    "iteration"
  ],
  "license": "BSD-3-Clause"
}
