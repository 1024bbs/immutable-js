{
  "name": "immutable",
  "version": "3.7.6",
  "description": "Immutable Data Collections",
  "homepage": "https://facebook.github.com/immutable-js",
  "author": {
    "name": "Lee Byron",
    "url": "https://github.com/leebyron"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/facebook/immutable-js.git"
  },
  "bugs": {
    "url": "https://github.com/facebook/immutable-js/issues"
  },
  "main": "dist/immutable.js",
  "typescript": {
    "definition": "dist/immutable.d.ts"
  },
  "scripts": {
    "test": "./resources/node_test.sh",
    "perf": "node ./resources/bench.js",
    "deploy": "(cd ./pages/out && git init && git config user.name \"Travis CI\" && git config user.email \"github@fb.com\" && git add . && git commit -m \"Deploy to GitHub Pages\" && git push --force --quiet \"https://${GH_TOKEN}@github.com/facebook/immutable-js.git\" master:gh-pages > /dev/null 2>1)"
  },
  "jest": {
    "scriptPreprocessor": "resources/jestPreprocessor.js",
    "testFileExtensions": [
      "js",
      "ts"
    ],
    "persistModuleRegistryBetweenSpecs": true
  },
  "devDependencies": {
    "acorn": "^0.11.0",
    "benchmark": "^1.0.0",
    "bluebird": "^2.7.1",
    "browser-sync": "^1.3.7",
    "browserify": "^5.11.2",
    "colors": "^1.0.3",
    "del": "^0.1.3",
    "es6-transpiler": "^0.7.18",
    "estraverse": "^1.9.1",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-jest": "^0.1.0",
    "grunt-release": "^0.7.0",
    "gulp": "^3.8.8",
    "gulp-concat": "^2.4.1",
    "gulp-filter": "^1.0.2",
    "gulp-header": "^1.1.1",
    "gulp-jest": "^0.2.1",
    "gulp-jshint": "^1.8.4",
    "gulp-less": "^1.3.5",
    "gulp-size": "^1.1.0",
    "gulp-sourcemaps": "^1.1.5",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "harmonize": "*",
    "jasmine-check": "^0.1.2",
    "jshint-stylish": "^0.4.0",
    "magic-string": "^0.2.6",
    "marked": "^0.3.2",
    "microtime": "^1.2.0",
    "node-jsx": "^0.12.4",
    "react": "^0.12.0",
    "react-router": "^0.11.2",
    "react-tools": "^0.12.0",
    "rollup": "^0.19.2",
    "run-sequence": "^0.3.7",
    "through2": "^0.6.1",
    "typescript": "~1.4.1",
    "uglify-js": "^2.4.15",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
